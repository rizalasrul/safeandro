<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
        style="@style/ParentLayoutFormStyle">

    <LinearLayout style="@style/ChildLayoutFormStyle">
        <include android:id="@+id/etBarcode" layout="@layout/component_edittext_barcode"/>
        <include android:id="@+id/etItemSample" layout="@layout/component_edittext"/>
        <include android:id="@+id/etProdBatch" layout="@layout/component_edittext"/>
        <include android:id="@+id/etIdMat" layout="@layout/component_edittext"/>
        <include android:id="@+id/etNamaMat" layout="@layout/component_edittext"/>

        <!--lotnum di hide dulu karena tidak digunakan-->
        <include android:id="@+id/etLotNum" layout="@layout/component_edittext" android:visibility="gone"/>
        <include android:id="@+id/etVol" layout="@layout/component_edittext"/>
        <include android:id="@+id/spTipeSample" layout="@layout/component_spinner"/>
        <include android:id="@+id/etItemORC" layout="@layout/component_edittext_multi"/>
        <include android:id="@+id/etStandard" layout="@layout/component_edittext"/>
        <include android:id="@+id/etHasil" layout="@layout/component_edittext"/>
        <include android:id="@+id/spHasil" android:visibility="gone" layout="@layout/component_spinner"/>
        <include android:id="@+id/btSaveBarcode" layout="@layout/component_button"/>
    </LinearLayout>
</ScrollView>